{
    "name": "@shqld/js-nodejs",
    "version": "1.0.0",
    "main": "build/main.js",
    "license": "UNLICENSED",
    "private": true,
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "start": "node --enable-source-maps build/main",
        "start:dry": "npm run start -- --dry",
        "build": "webpack-cli",
        "dev:build": "BUILD_ENV=development webpack-cli",
        "dev:watch": "npm run dev:build -- --watch",
        "test": "run-p test:*",
        "test:unit": "exit 0",
        "test:fmt": "prettier --check --loglevel warn .",
        "test:lint": "eslint --ext .ts .",
        "test:type": "tsc --noEmit",
        "fix": "run-s fix:*",
        "fix:fmt": "prettier --write .",
        "fix:lint": "eslint --ext .ts --fix ."
    },
    "dependencies": {
        "debug": "^4.3.3",
        "fastify": "^3.25.3",
        "fastify-compress": "^4.0.0",
        "fastify-etag": "^2.1.0",
        "fastify-plugin": "^3.0.0",
        "fastify-static": "^4.4.1",
        "undici": "^4.2.1"
    },
    "devDependencies": {
        "esbuild": "^0.14.11",
        "@babel/core": "^7.16.7",
        "@babel/cli": "^7.16.7",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
        "@babel/plugin-proposal-optional-chaining": "^7.16.7",
        "@babel/plugin-syntax-top-level-await": "^7.14.5",
        "@babel/preset-env": "^7.16.7",
        "@babel/preset-typescript": "^7.14.5",
        "@types/debug": "^4.1.7",
        "@types/webpack-node-externals": "^2.5.3",
        "@typescript-eslint/eslint-plugin": "^5.8.1",
        "@typescript-eslint/parser": "^5.8.1",
        "babel-loader": "^8.2.2",
        "compression-webpack-plugin": "^9.1.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon-webpack-plugin": "^4.5.2",
        "npm-run-all": "^4.1.5",
        "pino-pretty": "^7.2.0",
        "prettier": "^2.5.1",
        "typescript": "^4.3.5",
        "webpack": "^5.41.1",
        "webpack-cli": "^4.7.2",
        "webpack-manifest-plugin": "^4.0.2",
        "webpack-node-externals": "^3.0.0"
    }
}
